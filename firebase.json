{
  "hosting": {
    "public": "dist/prog-web-news",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "!/@(js|css|png)/**",
        "destination": "/index.html"
      }
    ]
  }
}
